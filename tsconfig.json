{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": [
    "components/**/*",
    "assets/**/*",
    "hooks/**/*",
    "constants/**/*",
    "utils/**/*",
    "ui/**/*",
    "images/**/*",
    "fonts/**/*",
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "config/firebaseConfig.js", "config/uploadImage.js"  ]
}
